{
  "name": "codexchat-workspace",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "quick": "make quick",
    "ci": "make ci",
    "swift:build": "bash scripts/swift-build-all.sh",
    "swift:test": "bash scripts/swift-test-all.sh",
    "biome:check": "biome ci .",
    "biome:fix": "biome check --write .",
    "knip": "bash scripts/knip.sh",
    "lint:web": "pnpm run biome:check && pnpm run knip",
    "check": "pnpm run swift:build && pnpm run swift:test && pnpm run lint:web"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "knip": "^5.83.1"
  }
}
